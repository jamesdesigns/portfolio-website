Change={set('phone')} className=\"contact-name\" type=\"phone\" id=\"phone\" name=\"phone\" />\n\t\t\t\t\t\t<label><p className=\"form-text-med\">How can I help you today?</p></label>\n\t\t\t\t\t\t<input value={values.goal} onChange={set('goal')} className=\"contact-name btm-margin-100\" type=\"goal\" id=\"goal\" name=\"goal\" required />\n\t\t\t\t\t</div>\n\t\t\t\t\t<div className=\"output-form\">\n\t\t\t\t\t\t<p><small>{values.name}</small></p>\n\t\t\t\t\t\t<p><small>{values.email}</small></p>\n\t\t\t\t\t\t<p><small>{values.phone}</small></p>\n\t\t\t\t\t\t<p><small>{values.goal}</small></p>\n\t\t\t\t\t\t<button className=\"btn btn--outline btn--large btn-submit mar-btm-30\" type=\"submit\">SUBMIT</button>\n\t\t\t\t\t</div>\n\t\t\t\t</form>\n\t\t\t\n      </div>\n    </div> \n    <Footer />\n    </>\n    )\n}","/Users/jameshooper/Desktop/PERSONAL/WebProjects/portfolio-website/src/components/pages/Portfolio.js",[],"/Users/jameshooper/Desktop/PERSONAL/WebProjects/portfolio-website/src/components/pages/Blog.js",[],"/Users/jameshooper/Desktop/PERSONAL/WebProjects/portfolio-website/src/components/Footer.js",[],"/Users/jameshooper/Desktop/PERSONAL/WebProjects/portfolio-website/src/components/Description.js",[],"/Users/jameshooper/Desktop/PERSONAL/WebProjects/portfolio-website/src/components/pages/HireMe.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"74","replacedBy":"78"},{"ruleId":"76","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":4,"column":10,"nodeType":"82","messageId":"83","endLine":4,"endColumn":14},{"ruleId":"80","severity":1,"message":"84","line":10,"column":8,"nodeType":"82","messageId":"83","endLine":10,"endColumn":14},{"ruleId":"80","severity":1,"message":"85","line":25,"column":9,"nodeType":"82","messageId":"83","endLine":25,"endColumn":17},{"ruleId":"86","severity":1,"message":"87","line":30,"column":20,"nodeType":"88","messageId":"89","endLine":30,"endColumn":56},"no-native-reassign",["90"],"no-n